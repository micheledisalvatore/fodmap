{
  "name": "fodmap",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
    "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
    "@fortawesome/react-fontawesome": "^0.1.0-11",
    "bootstrap": "^4.1.2",
    "diacritics": "^1.3.0",
    "prop-types": "^15.6.2",
    "ramda": "^0.25.0",
    "react": "^16.4.1",
    "react-click-outside": "^3.0.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.4",
    "reactstrap": "^6.3.0",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-saga": "^0.16.0",
    "styled-components": "^3.3.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "serve:start": "serve -s build -p 5000 &",
    "serve:stop": "if (lsof -t -i:5000); then kill $(lsof -t -i:5000); fi",
    "serve:stop-and-start": "npm-run-all serve:stop serve:start",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "serverless client deploy",
    "build-deploy": "npm-run-all build deploy",
    "lint": "eslint ./src --fix",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --config baseUrl=http://localhost:5000",
    "cypress:lint": "eslint -c ./cypress/.eslintrc --fix --ext .js ./cypress",
    "precommit": "CI=true npm-run-all -p test build"
  },
  "devDependencies": {
    "cypress": "3.0.2",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^0.14.3",
    "npm-run-all": "^4.1.3",
    "serverless-finch": "^2.2.0"
  }
}
